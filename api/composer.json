{
    "name": "Kitchen Kiosk",
    "description": "A family-centric household planning hub. Intended to be run either on a Rasperry Pi or at least through a touch-enabled screen via a browser on a Raspberry Pi.",
    "keywords": ["Kitchen Kiosk Planner"],
    "homepage": "http://kitchenkiosk.co",
    "license": "MIT",
    "authors": [{
        "name": "Cody Erekson",
        "role": "Developer",
        "email": "cody@erekson.org",
        "homepage": "http://codyerekson.me"
        }
    ],
    "require": {
        "php": ">= 5.5.0",
        "symfony/yaml": "~2.5",
        "monolog/monolog": "1.13.*@dev",
        "luracast/restler": "3.0.0-RC6",
        "luracast/explorer":"*",
        "hassankhan/config": "0.8.2",
        "swiftmailer/swiftmailer": "5.4.0",
        "ruudk/twitter-oauth": "1.0.*",
        "firebase/php-jwt": "^2.0",
        "bshaffer/oauth2-server-php":"v1.0",
        "filp/whoops": "^1.1",
        "maximebf/debugbar": ">=1.0.0",
        "guzzle/guzzle": "3.9.3",
        "php-di/php-di": "^5.0",
        "doctrine/cache": "^1.4",
        "doctrine/annotations": "^1.2",
        "ocramius/proxy-manager": "^1.0"
    },
    "repositories":[
        {
            "type":"package",
            "package":{
                "name":"luracast/explorer",
                "version":"v3.0.0",
                "dist":{
                    "type":"zip",
                    "url":"https://github.com/Luracast/Restler-API-Explorer/zipball/v3.0.0"
                }
            }
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "4.7.2"
    },
    "autoload": {
        "psr-4": {
            "KitchenKiosk\\": "vendor/Chillem/KitchenKiosk/"
        }
    }
}
